doctype html
html(lang="en-US")
	head
		include commonheaders
		title #{subtitle} | #{title}
		meta(name="description", content="Preamble about the guide's goals and requirements.")
		meta(property="og:description", content="Preamble about the guide's goals and requirements.")

	body
		include titlebar
		include pages/sidebarLeft
		div.content
			block content
			include pages/pagination

		include pages/sidebarRight